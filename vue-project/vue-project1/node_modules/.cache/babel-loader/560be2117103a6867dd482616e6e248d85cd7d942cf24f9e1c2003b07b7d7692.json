{"ast":null,"code":"import { ref, watch, onUnmounted } from 'vue';\nimport topNavigator from \"./firstPage/topNavigator.vue\";\nimport mainFooter from \"./firstPage/mainFooter.vue\";\nexport default {\n  components: {\n    topNavigator,\n    mainFooter\n  },\n  setup() {\n    const updateKey = ref(0);\n    const computedContext = ref([]);\n\n    // Watch for changes to document.searchLists\n    const unwatch = watch(() => document.searchLists, (newValue, oldValue) => {\n      console.log(`Value changed from ${oldValue} to ${newValue}`);\n      computedContext.value = newValue;\n      updateKey.value += 1;\n    }, {\n      deep: true\n    });\n\n    // Clean up the watcher when the component is unmounted\n    onUnmounted(unwatch);\n    return {\n      computedContext,\n      updateKey\n    };\n  }\n};","map":{"version":3,"names":["ref","watch","onUnmounted","topNavigator","mainFooter","components","setup","updateKey","computedContext","unwatch","document","searchLists","newValue","oldValue","console","log","value","deep"],"sources":["C:\\Users\\xuz21\\OneDrive\\Desktop\\2023 spring\\CSC331\\vueFrame\\vue-project\\vue-project1\\src\\components\\searchPage.vue"],"sourcesContent":["<template>\r\n  <topNavigator />\r\n  <div class=\"wrapper\">\r\n    <div class=\"whiteboard\">\r\n      <h2>Group lists</h2>\r\n      <ul>\r\n        <li v-for=\"(group) in computedContext\" :key=\"`group-${group.id}-${updateKey}`\">{{ group.name }}</li>\r\n      </ul>\r\n    </div>\r\n  </div>\r\n  <mainFooter />\r\n</template>\r\n\r\n<script>\r\nimport { ref, watch, onUnmounted } from 'vue';\r\nimport topNavigator from \"./firstPage/topNavigator.vue\";\r\nimport mainFooter from \"./firstPage/mainFooter.vue\";\r\n\r\nexport default {\r\n  components: {\r\n    topNavigator,\r\n    mainFooter\r\n  },\r\n  setup() {\r\n    const updateKey = ref(0);\r\n    const computedContext = ref([]);\r\n\r\n    // Watch for changes to document.searchLists\r\n    const unwatch = watch(() => document.searchLists, (newValue, oldValue) => {\r\n      console.log(`Value changed from ${oldValue} to ${newValue}`);\r\n      computedContext.value = newValue;\r\n      updateKey.value += 1;\r\n    }, { deep: true });\r\n\r\n    // Clean up the watcher when the component is unmounted\r\n    onUnmounted(unwatch);\r\n\r\n    return {\r\n      computedContext,\r\n      updateKey\r\n    };\r\n  }\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.wrapper {\r\n  height: 800px;\r\n  width: 100%;\r\n}\r\n\r\n.wrapper .whiteboard {\r\n  height: 600px;\r\n  width: 50%;\r\n  margin: 150px auto;\r\n  background-color: #fff;\r\n  box-shadow: 0 0 5px rgba(0, 0, 0, 0.3);\r\n}\r\n\r\n.whiteboard h2 {\r\n  margin-bottom: 20px;\r\n  /* top: 20px; */\r\n}\r\n</style>\r\n"],"mappings":"AAcA,SAASA,GAAG,EAAEC,KAAK,EAAEC,WAAU,QAAS,KAAK;AAC7C,OAAOC,YAAW,MAAO,8BAA8B;AACvD,OAAOC,UAAS,MAAO,4BAA4B;AAEnD,eAAe;EACbC,UAAU,EAAE;IACVF,YAAY;IACZC;EACF,CAAC;EACDE,KAAKA,CAAA,EAAG;IACN,MAAMC,SAAQ,GAAIP,GAAG,CAAC,CAAC,CAAC;IACxB,MAAMQ,eAAc,GAAIR,GAAG,CAAC,EAAE,CAAC;;IAE/B;IACA,MAAMS,OAAM,GAAIR,KAAK,CAAC,MAAMS,QAAQ,CAACC,WAAW,EAAE,CAACC,QAAQ,EAAEC,QAAQ,KAAK;MACxEC,OAAO,CAACC,GAAG,CAAE,sBAAqBF,QAAQ,OAAOD,QAAS,EAAC,CAAC;MAC5DJ,eAAe,CAACQ,KAAI,GAAIJ,QAAQ;MAChCL,SAAS,CAACS,KAAI,IAAK,CAAC;IACtB,CAAC,EAAE;MAAEC,IAAI,EAAE;IAAK,CAAC,CAAC;;IAElB;IACAf,WAAW,CAACO,OAAO,CAAC;IAEpB,OAAO;MACLD,eAAe;MACfD;IACF,CAAC;EACH;AACF,CAAC"},"metadata":{},"sourceType":"module","externalDependencies":[]}