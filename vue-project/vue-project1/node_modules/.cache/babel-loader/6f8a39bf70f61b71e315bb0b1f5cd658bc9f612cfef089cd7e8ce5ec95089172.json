{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport topNavigator from \"./firstPage/topNavigator.vue\";\nimport mainFooter from \"./firstPage/mainFooter.vue\";\nimport { useRouter } from 'vue-router';\nexport default {\n  setup() {\n    const router = useRouter();\n    function createGroup() {\n      router.push('/createGroup');\n    }\n    function joinGroup(groupId) {\n      // join group logic here\n      console.log(groupId);\n    }\n    components: {\n      topNavigator, mainFooter;\n    }\n    return {\n      createGroup,\n      joinGroup\n    };\n  },\n  data() {\n    return {\n      profileImage: \"path/to/profile-image.png\",\n      name: \"Your Name\",\n      skills: [\"Skill 1\", \"Skill 2\", \"Skill 3\"],\n      groups: [{\n        id: 1,\n        name: \"Group 1\",\n        image: \"https://via.placeholder.com/300x200.jpg\",\n        description: \"Description of Group 1\"\n      }, {\n        id: 2,\n        name: \"Group 2\",\n        image: \"https://via.placeholder.com/300x200.jpg\",\n        description: \"Description of Group 2\"\n      }, {\n        id: 3,\n        name: \"Group 3\",\n        image: \"https://via.placeholder.com/300x200.jpg\",\n        description: \"Description of Group 3\"\n      }]\n    };\n  }\n};","map":{"version":3,"names":["topNavigator","mainFooter","useRouter","setup","router","createGroup","push","joinGroup","groupId","console","log","components","data","profileImage","name","skills","groups","id","image","description"],"sources":["/Users/yizhu/Desktop/WFU Problem/WFU_Problems/vue-project/vue-project1/src/components/myGroups.vue"],"sourcesContent":["<template>\n    <topNavigator />\n    <div class=\"container\">\n        <!-- Profile column on the left -->\n        <div class=\"profile-column\">\n            <img src=\"https://via.placeholder.com/150\" alt=\"Profile Image\">\n            <h2>{{ name }}</h2>\n            <div class=\"skills\">\n                <span v-for=\"(skill, index) in skills\" :key=\"index\">{{ skill }}</span>\n            </div>\n        </div>\n\n        <div class=\"right-column\"> <!-- Create and Join buttons on the top right -->\n            <div class=\"buttons\">\n                <button @click=\"createGroup\">Create a Group</button>\n                <button @click=\"joinGroup\">Join by GID</button>\n            </div>\n\n            <!-- Group boxes in the center -->\n            <div class=\"groups\">\n                <div class=\"group-box\" v-for=\"(group, index) in groups\" :key=\"index\">\n                    <img :src=\"group.image\" alt=\"Group Image\">\n                    <h3>{{ group.name }}</h3>\n                    <p>{{ group.description }}</p>\n                    <button @click=\"joinGroup(group.id)\" class=\"invite\">Invite member</button>\n                </div>\n            </div>\n\n            <!-- Timeline block below groups box -->\n            <div class=\"timeline\">\n                <!-- timeline content here -->\n                <p class=\"time\">Timeline</p>\n                <!-- Example timeline demo -->\n                <div class=\"timeline-demo\">\n                    <div class=\"event\" data-date=\"January 2010\">\n                        <h3>Event 1</h3>\n                        <p>01/15/2023 Sprint1: Design the wireframes and the figma frames</p>\n                    </div>\n                    <div class=\"event\" data-date=\"March 2012\">\n                        <h3>Event 2</h3>\n                        <p>02/15/2023 Sprint2: Finish functions such as logging in or signing up</p>\n                    </div>\n                    <div class=\"event\" data-date=\"June 2015\">\n                        <h3>Event 3</h3>\n                        <p>03/15/2023 Sprint3: Build up the database and finish the data fetch function</p>\n                    </div>\n                    <div class=\"event\" data-date=\"December 2017\">\n                        <h3>Event 4</h3>\n                        <p>04/15/2023 Sprint4: Connect the frontends and backends, and take Unit testing</p>\n                    </div>\n                </div>\n\n            </div>\n        </div>\n    </div>\n    <mainFooter />\n</template>\n  \n<script>\nimport topNavigator from \"./firstPage/topNavigator.vue\"\nimport mainFooter from \"./firstPage/mainFooter.vue\"\nimport { useRouter } from 'vue-router';\nexport default {\nsetup () {\n    const router = useRouter();\n    function createGroup() {\n        router.push('/createGroup')\n    }\n    function joinGroup(groupId) {\n        // join group logic here\n        console.log(groupId)\n    }\n\n    components: {\n        topNavigator,\n        mainFooter\n    }\n    return{\n        createGroup,\n        joinGroup\n    }\n},\n\ndata() {\n        return {\n            profileImage: \"path/to/profile-image.png\",\n            name: \"Your Name\",\n            skills: [\"Skill 1\", \"Skill 2\", \"Skill 3\"],\n            groups: [\n                {\n                    id: 1,\n                    name: \"Group 1\",\n                    image: \"https://via.placeholder.com/300x200.jpg\",\n                    description: \"Description of Group 1\"\n                },\n                {\n                    id: 2,\n                    name: \"Group 2\",\n                    image: \"https://via.placeholder.com/300x200.jpg\",\n                    description: \"Description of Group 2\"\n                },\n                {\n                    id: 3,\n                    name: \"Group 3\",\n                    image: \"https://via.placeholder.com/300x200.jpg\",\n                    description: \"Description of Group 3\"\n                }\n            ]\n        };\n    },}\n</script>\n\n\n<style scoped>\n.container {\n    display: flex;\n    flex-direction: row;\n    width: 100%;\n    margin-top: 10px;\n    margin-bottom: 10px;\n}\n\n.profile-column {\n    width: 22%;\n    padding: 1rem;\n    box-sizing: border-box;\n    background-color: #f2f2f2;\n    height: 800px;\n    border-radius: 5px;\n    box-shadow: 0 0 3px 0 rgba(0, 0, 0, 0.4);\n    margin-left: 10px;\n}\n\n.profile-column img {\n    width: 80%;\n    max-width: 200px;\n    border-radius: 50%;\n    margin-top: 20px;\n}\n\n.profile-column h2{\n    margin-top: 20px;\n}\n\n.skills {\n    display: flex;\n    flex-direction: column;\n    /* change this property */\n    margin: 2rem auto;\n    width: 160px;\n}\n\n.skills span {\n    margin-bottom: 2rem;\n    /* add margin between the items */\n    background-color: #FF6969;\n    padding: 0.25rem 0.5rem;\n    border-radius: 1rem;\n}\n\n.right-column {\n    width: 76%;\n    height: 800px;\n    margin-left: 20px;\n    background-color: #f9f9f9;\n    border-radius: 5px;\n    box-shadow: 0 0 3px 0 rgba(0, 0, 0, 0.4);\n\n}\n\n.buttons {\n    width: 98%;\n    display: flex;\n    height: 60px;\n    /* flex-direction: row-reverse; */\n    /* padding: 10px; */\n    margin: 10px auto;\n    background-color: #f9f9f9;\n    border-radius: 5px;\n    box-shadow: 0 0 3px 0 rgba(0, 0, 0, 0.4);\n\n}\n\n.buttons button {\n    width: 250px;\n    height: 40px;\n    font-size: 20px;\n    font-weight: bold;\n    /* margin-left: 1rem; */\n    padding: 0.5rem 1rem;\n    border-radius: 1rem;\n    margin: 10px auto;\n    background-color: #ccc;\n    border: none;\n    cursor: pointer;\n    border-color: #000;\n    box-shadow: 0 0 3px 0 rgba(12, 5, 66, 0.7);\n}\n\n.groups {\n    width: 99%;\n    margin: 20px auto;\n    display: flex;\n    height: 440px;\n    /* flex-wrap: wrap; */\n    justify-content: space-between;\n    padding: 1rem;\n    box-sizing: border-box;\n    background-color: #f9f9f9;\n    border-radius: 5px;\n    box-shadow: 0 0 3px 0 rgba(0, 0, 0, 0.4);\n    /* flex-direction: row; */\n}\n\n\n.group-box {\n    width: 30%;\n    height: 400px;\n    margin-bottom: 2rem;\n    box-shadow: 0 4px 8px rgba(0, 0, 0, 0.1);\n    border-radius: 0.5rem;\n    overflow: hidden;\n}\n\n.group-box .invite {\n    font-weight: bold;\n}\n\n.group-box img {\n    width: 100%;\n    height: 200px;\n    object-fit: cover;\n}\n\n.group-box h3 {\n    margin: 1rem;\n}\n\n.group-box p {\n    margin: 0 1rem 1rem 1rem;\n}\n\n.group-box button {\n    margin: 1rem;\n    padding: 0.5rem 1rem;\n    border-radius: 1rem;\n    background-color: #ccc;\n    border: none;\n    cursor: pointer;\n}\n\n.timeline {\n    width: 99%;\n    padding-top: 10px;\n    margin: 20px auto;\n    height: 230px;\n    /* padding: 1rem; */\n    /* box-sizing: border-box; */\n    background-color: #f9f9f9;\n    border-radius: 5px;\n    box-shadow: 0 0 3px 0 rgba(0, 0, 0, 0.4);\n}\n\n.timeline .time {\n    font-size: 25px;\n    font-weight: bold;\n    text-decoration: underline;\n}\n\n.event{\n    padding-top: 7px;\n}\n</style>\n"],"mappings":";AA2DA,OAAOA,YAAW,MAAO,8BAA6B;AACtD,OAAOC,UAAS,MAAO,4BAA2B;AAClD,SAASC,SAAQ,QAAS,YAAY;AACtC,eAAe;EACfC,KAAIA,CAAA,EAAK;IACL,MAAMC,MAAK,GAAIF,SAAS,EAAE;IAC1B,SAASG,WAAWA,CAAA,EAAG;MACnBD,MAAM,CAACE,IAAI,CAAC,cAAc;IAC9B;IACA,SAASC,SAASA,CAACC,OAAO,EAAE;MACxB;MACAC,OAAO,CAACC,GAAG,CAACF,OAAO;IACvB;IAEAG,UAAU,EAAE;MACRX,YAAY,EACZC,UAAS;IACb;IACA,OAAM;MACFI,WAAW;MACXE;IACJ;EACJ,CAAC;EAEDK,IAAIA,CAAA,EAAG;IACC,OAAO;MACHC,YAAY,EAAE,2BAA2B;MACzCC,IAAI,EAAE,WAAW;MACjBC,MAAM,EAAE,CAAC,SAAS,EAAE,SAAS,EAAE,SAAS,CAAC;MACzCC,MAAM,EAAE,CACJ;QACIC,EAAE,EAAE,CAAC;QACLH,IAAI,EAAE,SAAS;QACfI,KAAK,EAAE,yCAAyC;QAChDC,WAAW,EAAE;MACjB,CAAC,EACD;QACIF,EAAE,EAAE,CAAC;QACLH,IAAI,EAAE,SAAS;QACfI,KAAK,EAAE,yCAAyC;QAChDC,WAAW,EAAE;MACjB,CAAC,EACD;QACIF,EAAE,EAAE,CAAC;QACLH,IAAI,EAAE,SAAS;QACfI,KAAK,EAAE,yCAAyC;QAChDC,WAAW,EAAE;MACjB;IAER,CAAC;EACL;AAAE"},"metadata":{},"sourceType":"module","externalDependencies":[]}