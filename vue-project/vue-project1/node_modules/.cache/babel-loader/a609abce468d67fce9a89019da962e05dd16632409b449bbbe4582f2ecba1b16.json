\"\r\n                }\r\n            ]\r\n        };\r\n    },\r\n    methods: {\r\n        createGroup() {\r\n            // create group logic here\r\n        },\r\n        joinGroup(groupId) {\r\n            // join group logic here\r\n            console.log(groupId)\r\n        }\r\n    }\r\n};\r\n</script>\r\n\r\n<style>\r\n.container {\r\n    display: flex;\r\n    flex-direction: row;\r\n    width: 100%;\r\n}\r\n\r\n.profile-column {\r\n    width: 22%;\r\n    padding: 1rem;\r\n    box-sizing: border-box;\r\n    background-color: #f2f2f2;\r\n    height: 800px;\r\n    border-radius: 5px;\r\n    box-shadow: 0 0 3px 0 rgba(0, 0, 0, 0.4);\r\n    margin-left: 10px;\r\n}\r\n\r\n.profile-column img {\r\n    width: 100%;\r\n    max-width: 200px;\r\n    border-radius: 50%;\r\n}\r\n\r\n.skills {\r\n    display: flex;\r\n    flex-direction: column;\r\n    /* change this property */\r\n    margin-top: 1rem;\r\n}\r\n\r\n.skills span {\r\n    margin-bottom: 0.5rem;\r\n    /* add margin between the items */\r\n    background-color: #eee;\r\n    padding: 0.25rem 0.5rem;\r\n    border-radius: 1rem;\r\n}\r\n.right-column{\r\n    width: 76%;\r\n    height: 800px;\r\n    margin-left: 20px;\r\n    background-color: #f9f9f9;\r\n    border-radius: 5px;\r\n    box-shadow: 0 0 3px 0 rgba(0, 0, 0, 0.4);\r\n\r\n}\r\n.right-column .buttons {\r\n    width: 99%;\r\n    display: flex;\r\n    height: 80px;\r\n    /* flex-direction: row-reverse; */\r\n    padding: 10px;\r\n    background-color: #f9f9f9;\r\n    border-radius: 5px;\r\n    box-shadow: 0 0 3px 0 rgba(0, 0, 0, 0.4);\r\n    \r\n    /* box-sizing: border-box; */\r\n}\r\n\r\n.buttons button {\r\n    width: 250px;\r\n    height: 60px;\r\n    font-size: 22px;\r\n    font-weight: bold;\r\n    /* margin-left: 1rem; */\r\n    padding: 0.5rem 1rem;\r\n    border-radius: 1rem;\r\n    margin: 0 auto;\r\n    background-color: #ccc;\r\n    border: none;\r\n    cursor: pointer;\r\n}\r\n\r\n.groups {\r\n    width: 99%;\r\n    margin: 8px;\r\n    display: flex;\r\n    /* flex-wrap: wrap; */\r\n    justify-content: space-between;\r\n    padding: 1rem;\r\n    box-sizing: border-box;\r\n    background-color: #f9f9f9;\r\n    border-radius: 5px;\r\n    box-shadow: 0 0 3px 0 rgba(0, 0, 0, 0.4);\r\n}\r\n\r\n.group-box {\r\n    width: 30%;\r\n    margin-bottom: 2rem;\r\n    box-shadow: 0 4px 8px rgba(0, 0, 0, 0.1);\r\n    border-radius: 0.5rem;\r\n    overflow: hidden;\r\n}\r\n\r\n.group-box img {\r\n    width: 100%;\r\n    height: 200px;\r\n    object-fit: cover;\r\n}\r\n\r\n.group-box h3 {\r\n    margin: 1rem;\r\n}\r\n\r\n.group-box p {\r\n    margin: 0 1rem 1rem 1rem;\r\n}\r\n\r\n.group-box button {\r\n    margin: 1rem;\r\n    padding: 0.5rem 1rem;\r\n    border-radius: 1rem;\r\n    background-color: #ccc;\r\n    border: none;\r\n    cursor: pointer;\r\n}\r\n\r\n.timeline {\r\n    width: 99%;\r\n    padding-top: 10px;\r\n    margin: 0px 10 0 0 auto;\r\n    height: 200px;\r\n    /* padding: 1rem; */\r\n    /* box-sizing: border-box; */\r\n    background-color: #f9f9f9;\r\n    border-radius: 5px;\r\n    box-shadow: 0 0 3px 0 rgba(0, 0, 0, 0.4);\r\n}</style>\r\n"],"mappings":";;EACSA,KAAK,EAAC;AAAW;;EAEbA,KAAK,EAAC;AAAgB;gCACvBC,mBAAA,CAA+D;EAA1DC,GAAG,EAAC,iCAAiC;EAACC,GAAG,EAAC;;;EAE1CH,KAAK,EAAC;AAAQ;;EAKlBA,KAAK,EAAC;AAAc;;EAChBA,KAAK,EAAC;AAAS;;EAMfA,KAAK,EAAC;AAAQ;;;iCAUnBC,mBAAA,CAEM;EAFDD,KAAK,EAAC;AAAU,I,aACjBI,mBAAA,2BAA8B,C;;;uBA5B1CC,mBAAA,CAgCM,OAhCNC,UAgCM,GA/BFF,mBAAA,gCAAmC,EACnCH,mBAAA,CAMM,OANNM,UAMM,GALFC,UAA+D,EAC/DP,mBAAA,CAAmB,YAAAQ,gBAAA,CAAZC,KAAA,CAAAC,IAAI,kBACXV,mBAAA,CAEM,OAFNW,UAEM,I,kBADFP,mBAAA,CAAsEQ,SAAA,QAAAC,WAAA,CAAvCJ,KAAA,CAAAK,MAAM,GAAvBC,KAAK,EAAEC,KAAK;yBAA1BZ,mBAAA,CAAsE;MAA9Ba,GAAG,EAAED;IAAK,GAAAR,gBAAA,CAAKO,KAAK;sCAIpEf,mBAAA,CAoBM,OApBNkB,UAoBM,GApBqBf,mBAAA,8CAAiD,EACxEH,mBAAA,CAGM,OAHNmB,UAGM,GAFFnB,mBAAA,CAAoD;IAA3CoB,OAAK,EAAAC,MAAA,QAAAA,MAAA,UAAAC,IAAA,KAAEC,QAAA,CAAAC,WAAA,IAAAD,QAAA,CAAAC,WAAA,IAAAF,IAAA,CAAW;KAAE,gBAAc,GAC3CtB,mBAAA,CAA+C;IAAtCoB,OAAK,EAAAC,M