n.menu ul li:hover>ul {\r\n    display: block;\r\n    opacity: 1;\r\n}\r\n\r\n\r\n/* .menu ul li ul li {\r\n    display: block;\r\n} */\r\n\r\n\r\n.menu ul li ul li a {\r\n    padding: 20px 15px;\r\n    color: #fff;\r\n    font-size: 16px;\r\n    font-weight: bold;\r\n    text-decoration: none;\r\n    display: block;\r\n}\r\n\r\n.menu ul li ul li a:hover {\r\n    background-color: #555;\r\n    color: #fff;\r\n}\r\n\r\n.headBar .site-search {\r\n    float: right;\r\n    width: 440px;\r\n    margin-top: 5px;\r\n    margin-left: 30px;\r\n    /* background-color: #777; */\r\n}\r\n\r\n.headBar .site-search from {\r\n    position: relative;\r\n    width: 400px;\r\n    height: 40px;\r\n}\r\n\r\n.search-text {\r\n    width: 300px;\r\n    height: 38px;\r\n    border: 1px solid #E0E0E0;\r\n    font-size: 15px;\r\n    padding: 0 10px;\r\n    float: left;\r\n    background-color: lightgray;\r\n}\r\n\r\n.submit {\r\n    float: left;\r\n    width: 100px;\r\n    height: 40px;\r\n    /* margin-top: 2px; */\r\n    margin-left: -1px;\r\n    border: 0px solid #E0E0E0;\r\n    font-size: 16px;\r\n    font-weight: bold;\r\n    color: white;\r\n    /* background: linear-gradient(100deg, #e0c3fc 0%, #8ec5fc 100%); */\r\n    background-color: #577D86;\r\n}\r\n\r\n.submit:hover {\r\n    /* background-color: #555; */\r\n    color: #B9EDDD;\r\n}\r\n\r\n.login {\r\n    float: right;\r\n    /* margin: 0 20px 0 0; */\r\n    /* color: #FF7F50; */\r\n\r\n}\r\n\r\n.login img {\r\n    width: 40px;\r\n    padding-top: 5px;\r\n}\r\n\r\n.login a {\r\n    color: #F6F6F6;\r\n}\r\n\r\n.login a:hover {\r\n    color: #FF7F50;\r\n}</style>\r\n"],"mappings":"AA2CA,SAASA,WAAWA,CAACC,GAAG,EAAE;EACtB,IAAIA,GAAG,CAACC,GAAG,CAACC,KAAK,CAAC,YAAY,CAAC,EAAE;IAC7BF,GAAG,CAACC,GAAE,GAAI,YAAY;EAC1B,OAAO;IACHD,GAAG,CAACC,GAAE,GAAI,YAAY;EAC1B;AACJ"},"metadata":{},"sourceType":"script","externalDependencies":[]}