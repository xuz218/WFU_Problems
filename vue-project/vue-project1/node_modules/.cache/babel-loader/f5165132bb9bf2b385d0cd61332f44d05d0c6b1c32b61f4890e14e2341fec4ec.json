t\" href=\"https://use.fontawesome.com/releases/v6.1.1/css/all.css\">\n  <div class=\"profile\">\n    <div class=\"header\">\n      <div class=\"avatar\">\n        <img :src=\"user.avatar\" alt=\"User Avatar\" />\n      </div>\n      <h1>{{ user.name }}</h1>\n      <p>{{ user.title }}</p>\n      <div class=\"social-links\">\n        <a :href=\"user.socialLinks.twitter\"><i class=\"fab fa-twitter\"></i></a>\n        <a :href=\"user.socialLinks.github\"><i class=\"fab fa-github\"></i></a>\n        <a :href=\"user.socialLinks.linkedin\"><i class=\"fab fa-linkedin\"></i></a>\n      </div>\n    </div>\n    <div class=\"content\">\n      <h2>About Me</h2>\n      <p>{{ user.about }}</p>\n\n      <h2>Email</h2>\n      <p>{{ user.email }}</p>\n\n      <h2>Skills</h2>\n      <ul>\n        <li v-for=\"(skill, index) in user.skills\" :key=\"index\">{{ skill }}</li>\n      </ul>\n\n      <h2>Introduction</h2>\n      <p>{{ user.introduction }}</p>\n\n      <h2>Interests</h2>\n      <ul>\n        <li v-for=\"(interest, index) in user.interests\" :key=\"index\">{{ interest }}</li>\n      </ul>\n\n      <h2>Group lists</h2>\n      <li v-for=\"(group, index) in user.groups\" :key=\"index\">{{ group }}</li>\n\n\n\n\n\n      <!-- <h2>Experience</h2>\n      <ul>\n        <li v-for=\"(experience, index) in user.experience\" :key=\"index\">\n          <h3>{{ experience.position }}</h3>\n          <p>{{ experience.company }}</p>\n          <p>{{ experience.date }}</p>\n          <ul>\n            <li v-for=\"(task, index) in experience.tasks\" :key=\"index\">{{ task }}</li>\n          </ul>\n        </li>\n      </ul> -->\n    </div>\n  </div>\n  <div class=\"container\">\n    <button class=\"post\">Upload Projects</button>\n  </div>\n  <mainFooter />\n</template>\n\n  \n<script>\nimport topNavigator from \"./firstPage/topNavigator.vue\"\nimport mainFooter from \"./firstPage/mainFooter.vue\"\n\nexport default {\n  data() {\n    return {\n      user: {\n        name: document.userProfile.username,\n        title: \"Web Developer\",\n        avatar: \"https://via.placeholder.com/150\",\n        socialLinks: {\n          twitter: \"https://twitter.com\",\n          github: \"https://github.com\",\n          linkedin: \"https://linkedin.com\",\n        },\n        email: document.userProfile.email,\n        about:\n          \"Open to work\",\n        introduction: document.userProfile.introduction,\n        skills: document.userProfile.skill,\n        interests: document.userProfile.interest,\n        groups: document.userProfile.group_list,\n        experience: [\n          {\n            position: \"Web Developer\",\n            company: \"Wake Forest University\",\n            date: \"Jan 2023 - Present\",\n            tasks: [\n              \"- Developed and maintained the company website\",\n              \"- Optimized website performance and speed\",\n              \"- Collaborated with designers and other developers on projects\",\n            ],\n          },\n          {\n            position: \"Frontend Developer\",\n            company: \"Wake Forest University\",\n            date: \"Jan 2023 - Present\",\n            tasks: [\n              \"- Built and maintained the WFU problems project\",\n              \"- Worked on various frontend projects\",\n            ],\n          },\n        ],\n      },\n    };\n  },\n\n  components: {\n    topNavigator,\n    mainFooter\n  }\n};\n</script>\n  \n<style>\n.profile {\n  max-width: 800px;\n  margin: 30px auto;\n  /* background-color: black; */\n  padding-bottom: 10px;\n}\n\n.header {\n  display: flex;\n  flex-direction: column;\n  align-items: center;\n  text-align: center;\n  margin-bottom: 2rem;\n}\n\n.avatar {\n  width: 150px;\n  height: 150px;\n  border-radius: 50%;\n  overflow: hidden;\n  margin-bottom: 1rem;\n}\n\n.avatar img {\n  width: 100%;\n  height: 100%;\n  object-fit: cover;\n}\n\n.social-links {\n  display: flex;\n  justify-content: center;\n  margin-top: 1rem;\n}\n\n.social-links a {\n  display: inline-block;\n  margin-right: 1rem;\n  font-size: 1.5rem;\n  color: #666;\n  transition: all 0.2s ease-in-out;\n}\n\n.social-links a:hover {\n  color: #007bff;\n}\n\n.content {\n  padding: 2rem;\n  border-radius: 5px;\n  background-color: #fff;\n  box-shadow: 0px 0px 10px rgba(0, 0, 0, 0.2);\n}\n\n.content h2 {\n  margin-top: 1rem;\n  margin-bottom: 0.2rem;\n  font-size: 1.5rem;\n}\n\n.content ul {\n  margin: 0;\n  padding: 0;\n  list-style: none;\n}\n\n.content li {\n  margin-bottom: 0.5rem;\n}\n\n.content h3 {\n  margin-top: 0;\n  margin-bottom: 0.5rem;\n  font-size: 1.25rem;\n}\n\n.container {\n  text-align: center;\n}\n\n.post {\n  margin: 0 auto;\n  width: 180px;\n  height: 50px;\n  border-radius: 5px;\n  border-color: #F0F0F0;\n  margin-bottom: 20px;\n  font-weight: bold;\n  color: #F9D949;\n  font-size: 20px;\n  background-color: #3C486B;\n  box-shadow: 0 0 5px rgba(0, 0, 0, 0.3);\n}\n</style>\n  "],"mappings":"AAgEA,OAAOA,YAAW,MAAO,8BAA6B;AACtD,OAAOC,UAAS,MAAO,4BAA2B;AAElD,eAAe;EACbC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,IAAI,EAAE;QACJC,IAAI,EAAEC,QAAQ,CAACC,WAAW,CAACC,QAAQ;QACnCC,KAAK,EAAE,eAAe;QACtBC,MAAM,EAAE,iCAAiC;QACzCC,WAAW,EAAE;UACXC,OAAO,EAAE,qBAAqB;UAC9BC,MAAM,EAAE,oBAAoB;UAC5BC,QAAQ,EAAE;QACZ,CAAC;QACDC,KAAK,EAAET,QAAQ,CAACC,WAAW,CAACQ,KAAK;QACjCC,KAAK,EACH,cAAc;QAChBC,YAAY,EAAEX,QAAQ,CAACC,WAAW,CAACU,YAAY;QAC/CC,MAAM,EAAEZ,QAAQ,CAACC,WAAW,CAACY,KAAK;QAClCC,SAAS,EAAEd,QAAQ,CAACC,WAAW,CAACc,QAAQ;QACxCC,MAAM,EAAEhB,QAAQ,CAACC,WAAW,CAACgB,UAAU;QACvCC,UAAU,EAAE,CACV;UACEC,QAAQ,EAAE,eAAe;UACzBC,OAAO,EAAE,wBAAwB;UACjCC,IAAI,EAAE,oBAAoB;UAC1BC,KAAK,EAAE,CACL,gDAAgD,EAChD,2CAA2C,EAC3C,gEAAgE;QAEpE,CAAC,EACD;UACEH,QAAQ,EAAE,oBAAoB;UAC9BC,OAAO,EAAE,wBAAwB;UACjCC,IAAI,EAAE,oBAAoB;UAC1BC,KAAK,EAAE,CACL,iDAAiD,EACjD,uCAAuC;QAE3C,CAAC;MAEL;IACF,CAAC;EACH,CAAC;EAEDC,UAAU,EAAE;IACV5B,YAAY;IACZC;EACF;AACF,CAAC"},"metadata":{},"sourceType":"module","externalDependencies":[]}