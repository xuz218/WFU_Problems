{"ast":null,"code":"import topNavigator from \"./firstPage/topNavigator.vue\";\nimport mainFooter from \"./firstPage/mainFooter.vue\";\nexport default {\n  data() {\n    return {\n      user: {\n        name: document.userProfile.username,\n        title: \"Web Developer\",\n        avatar: \"https://via.placeholder.com/150\",\n        socialLinks: {\n          twitter: \"https://twitter.com\",\n          github: \"https://github.com\",\n          linkedin: \"https://linkedin.com\"\n        },\n        email: document.userProfile.email,\n        about: \"Open to work\",\n        introduction: document.userProfile.introduction,\n        skills: document.userProfile.skill,\n        interests: document.userProfile.interest,\n        groups: document.userProfile.group_list,\n        experience: [{\n          position: \"Web Developer\",\n          company: \"Wake Forest University\",\n          date: \"Jan 2023 - Present\",\n          tasks: [\"- Developed and maintained the company website\", \"- Optimized website performance and speed\", \"- Collaborated with designers and other developers on projects\"]\n        }, {\n          position: \"Frontend Developer\",\n          company: \"Wake Forest University\",\n          date: \"Jan 2023 - Present\",\n          tasks: [\"- Built and maintained the WFU problems project\", \"- Worked on various frontend projects\"]\n        }]\n      }\n    };\n  },\n  components: {\n    topNavigator,\n    mainFooter\n  }\n};","map":{"version":3,"names":["topNavigator","mainFooter","data","user","name","document","userProfile","username","title","avatar","socialLinks","twitter","github","linkedin","email","about","introduction","skills","skill","interests","interest","groups","group_list","experience","position","company","date","tasks","components"],"sources":["C:\\Users\\xuz21\\OneDrive\\Desktop\\2023 spring\\CSC331\\vueFrame\\vue-project\\vue-project1\\src\\components\\profilePage.vue"],"sourcesContent":["<template>\r\n  <topNavigator/>\r\n  <link rel=\"stylesheet\" href=\"https://use.fontawesome.com/releases/v6.1.1/css/all.css\">\r\n  <div class=\"profile\">\r\n    <div class=\"header\">\r\n      <div class=\"avatar\">\r\n        <img :src=\"user.avatar\" alt=\"User Avatar\" />\r\n      </div>\r\n      <h1>{{ user.name }}</h1>\r\n      <p>{{ user.title }}</p>\r\n      <div class=\"social-links\">\r\n        <a :href=\"user.socialLinks.twitter\"><i class=\"fab fa-twitter\"></i></a>\r\n        <a :href=\"user.socialLinks.github\"><i class=\"fab fa-github\"></i></a>\r\n        <a :href=\"user.socialLinks.linkedin\"><i class=\"fab fa-linkedin\"></i></a>\r\n      </div>\r\n    </div>\r\n    <div class=\"content\">\r\n      <h2>About Me</h2>\r\n      <p>{{ user.about }}</p>\r\n\r\n      <h2>Email</h2>\r\n      <p>{{ user.email }}</p>\r\n\r\n      <h2>Skills</h2>\r\n      <ul>\r\n        <li v-for=\"(skill, index) in user.skills\" :key=\"index\">{{ skill }}</li>\r\n      </ul>\r\n\r\n      <h2>Introduction</h2>\r\n      <p>{{ user.introduction }}</p>\r\n\r\n      <h2>Interests</h2>\r\n      <ul>\r\n        <li v-for=\"(interest, index) in user.interests\" :key=\"index\">{{ interest }}</li>\r\n      </ul>\r\n\r\n      <h2>Group lists</h2>\r\n      \r\n\r\n      <!-- <h2>Experience</h2>\r\n      <ul>\r\n        <li v-for=\"(experience, index) in user.experience\" :key=\"index\">\r\n          <h3>{{ experience.position }}</h3>\r\n          <p>{{ experience.company }}</p>\r\n          <p>{{ experience.date }}</p>\r\n          <ul>\r\n            <li v-for=\"(task, index) in experience.tasks\" :key=\"index\">{{ task }}</li>\r\n          </ul>\r\n        </li>\r\n      </ul> -->\r\n    </div>\r\n  </div>\r\n  <mainFooter/>\r\n</template>\r\n\r\n  \r\n<script>\r\nimport topNavigator from \"./firstPage/topNavigator.vue\"\r\nimport mainFooter from \"./firstPage/mainFooter.vue\"\r\n\r\nexport default {\r\n  data() {\r\n    return {\r\n      user: {\r\n        name: document.userProfile.username,\r\n        title: \"Web Developer\",\r\n        avatar: \"https://via.placeholder.com/150\",\r\n        socialLinks: {\r\n          twitter: \"https://twitter.com\",\r\n          github: \"https://github.com\",\r\n          linkedin: \"https://linkedin.com\",\r\n        },\r\n        email: document.userProfile.email,\r\n        about:\r\n          \"Open to work\",\r\n        introduction: document.userProfile.introduction,\r\n        skills: document.userProfile.skill,\r\n        interests: document.userProfile.interest,\r\n        groups: document.userProfile.group_list,\r\n        experience: [\r\n          {\r\n            position: \"Web Developer\",\r\n            company: \"Wake Forest University\",\r\n            date: \"Jan 2023 - Present\",\r\n            tasks: [\r\n              \"- Developed and maintained the company website\",\r\n              \"- Optimized website performance and speed\",\r\n              \"- Collaborated with designers and other developers on projects\",\r\n            ],\r\n          },\r\n          {\r\n            position: \"Frontend Developer\",\r\n            company: \"Wake Forest University\",\r\n            date: \"Jan 2023 - Present\",\r\n            tasks: [\r\n              \"- Built and maintained the WFU problems project\",\r\n              \"- Worked on various frontend projects\",\r\n            ],\r\n          },\r\n        ],\r\n      },\r\n    };\r\n  },\r\n\r\n  components:{\r\n    topNavigator,\r\n    mainFooter\r\n  }\r\n};\r\n</script>\r\n  \r\n<style>\r\n.profile {\r\n  max-width: 800px;\r\n  margin: 30px auto;\r\n  /* background-color: black; */\r\n  padding-bottom: 20px;\r\n}\r\n\r\n.header {\r\n  display: flex;\r\n  flex-direction: column;\r\n  align-items: center;\r\n  text-align: center;\r\n  margin-bottom: 2rem;\r\n}\r\n\r\n.avatar {\r\n  width: 150px;\r\n  height: 150px;\r\n  border-radius: 50%;\r\n  overflow: hidden;\r\n  margin-bottom: 1rem;\r\n}\r\n\r\n.avatar img {\r\n  width: 100%;\r\n  height: 100%;\r\n  object-fit: cover;\r\n}\r\n\r\n.social-links {\r\n  display: flex;\r\n  justify-content: center;\r\n  margin-top: 1rem;\r\n}\r\n\r\n.social-links a {\r\n  display: inline-block;\r\n  margin-right: 1rem;\r\n  font-size: 1.5rem;\r\n  color: #666;\r\n  transition: all 0.2s ease-in-out;\r\n}\r\n\r\n.social-links a:hover {\r\n  color: #007bff;\r\n}\r\n\r\n.content {\r\n  padding: 2rem;\r\n  border-radius: 5px;\r\n  background-color: #fff;\r\n  box-shadow: 0px 0px 10px rgba(0, 0, 0, 0.2);\r\n}\r\n\r\n.content h2 {\r\n  margin-top: 1rem;\r\n  margin-bottom: 0rem;\r\n  font-size: 1.5rem;\r\n}\r\n\r\n.content ul {\r\n  margin: 0;\r\n  padding: 0;\r\n  list-style: none;\r\n}\r\n\r\n.content li {\r\n  margin-bottom: 0.5rem;\r\n}\r\n\r\n.content h3 {\r\n  margin-top: 0;\r\n  margin-bottom: 0.5rem;\r\n  font-size: 1.25rem;\r\n}\r\n</style>\r\n  "],"mappings":"AAyDA,OAAOA,YAAW,MAAO,8BAA6B;AACtD,OAAOC,UAAS,MAAO,4BAA2B;AAElD,eAAe;EACbC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,IAAI,EAAE;QACJC,IAAI,EAAEC,QAAQ,CAACC,WAAW,CAACC,QAAQ;QACnCC,KAAK,EAAE,eAAe;QACtBC,MAAM,EAAE,iCAAiC;QACzCC,WAAW,EAAE;UACXC,OAAO,EAAE,qBAAqB;UAC9BC,MAAM,EAAE,oBAAoB;UAC5BC,QAAQ,EAAE;QACZ,CAAC;QACDC,KAAK,EAAET,QAAQ,CAACC,WAAW,CAACQ,KAAK;QACjCC,KAAK,EACH,cAAc;QAChBC,YAAY,EAAEX,QAAQ,CAACC,WAAW,CAACU,YAAY;QAC/CC,MAAM,EAAEZ,QAAQ,CAACC,WAAW,CAACY,KAAK;QAClCC,SAAS,EAAEd,QAAQ,CAACC,WAAW,CAACc,QAAQ;QACxCC,MAAM,EAAEhB,QAAQ,CAACC,WAAW,CAACgB,UAAU;QACvCC,UAAU,EAAE,CACV;UACEC,QAAQ,EAAE,eAAe;UACzBC,OAAO,EAAE,wBAAwB;UACjCC,IAAI,EAAE,oBAAoB;UAC1BC,KAAK,EAAE,CACL,gDAAgD,EAChD,2CAA2C,EAC3C,gEAAgE;QAEpE,CAAC,EACD;UACEH,QAAQ,EAAE,oBAAoB;UAC9BC,OAAO,EAAE,wBAAwB;UACjCC,IAAI,EAAE,oBAAoB;UAC1BC,KAAK,EAAE,CACL,iDAAiD,EACjD,uCAAuC;QAE3C,CAAC;MAEL;IACF,CAAC;EACH,CAAC;EAEDC,UAAU,EAAC;IACT5B,YAAY;IACZC;EACF;AACF,CAAC"},"metadata":{},"sourceType":"module","externalDependencies":[]}